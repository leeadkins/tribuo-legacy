<!DOCTYPE html>
<html>
<head>
  <title>Tribuo 2010</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body> 
	<div id="content">
		<div id="header">
			<h1>Tribuo</h1>
		</div>
		<div id="navigation">
			<%= render_navigation :context => :main %>
		</div>
		<div>
			<%= content_for?(:content) ? yield(:content) : yield %>
		</div>
	</div>
</body>
</html>